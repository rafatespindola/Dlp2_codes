vsim work.timer_comp
# vsim work.timer_comp 
# Loading std.standard
# Loading std.textio(body)
# Loading ieee.std_logic_1164(body)
# Loading ieee.numeric_std(body)
# Loading work.timer_comp(rtl)
# Loading work.count59(rtl)
# Loading work.count23(rtl)
add wave sim:/timer_comp/*
force -freeze sim:/timer_comp/clk 1 0, 0 {25 ps} -r 50
force -freeze sim:/timer_comp/reset 1 0
run 10ns
restart
force -freeze sim:/timer_comp/clk 1 0, 0 {25 ps} -r 50
force -freeze sim:/timer_comp/reset 1 0
run 10ps
run 190ps
noforce sim:/timer_comp/reset
force -freeze sim:/timer_comp/reset 0 0
run 100ns
restart
force -freeze sim:/timer_comp/clk 1 0, 0 {50 ps} -r 100
force -freeze sim:/timer_comp/reset 1 0
run 200ps
force -freeze sim:/timer_comp/reset 0 0
run 200ps
run 10000ps
